{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["<CR>"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["*"],
      "after": ["*", "`", "`"]
    }
  ],
  "vim.useSystemClipboard": true,
  // "vim.vimrc.enable": true,
  // "vim.vimrc.path": "~/.vimrc",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.renderControlCharacters": true,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.fontFamily": "HackGen35, Menlo, Monaco, 'Courier New', monospace",
  "diffEditor.renderSideBySide": false,
  "editor.formatOnPaste": true,
  "files.associations": {
    ".textlintrc": "json"
  },
  "workbench.iconTheme": "vscode-icons",
  "dart.flutterSdkPath": "/Users/shinjikaneko/flutter",
  "salesforcedx-vscode-apex.java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home",
  "cSpell.userWords": ["kintone"],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.colorTheme": "GitHub Dark Default",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.inlineSuggest.enabled": true
}
